<h1>Listing meal_times</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meal_times.each do |meal_time| %>
      <tr>
        <td><%= link_to 'Show', meal_time %></td>
        <td><%= link_to 'Edit', edit_meal_time_path(meal_time) %></td>
        <td><%= link_to 'Destroy', meal_time, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Meal time', new_meal_time_path %>
